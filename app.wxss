/*边框设置*/
.border-around,
.border-bottom-1px,
.border-left-1px,
.border-right-1px,
.border-top-1px {
  position: relative;
  border: 1rpx solid #ededed;
}
.border-top-1px {
  border-width: 1rpx 0 0 0;
}
.border-right-1px {
  border-width: 0 1rpx 0 0;
}
.border-bottom-1px {
  border-width: 0 0 1rpx 0;
}
.border-left-1px {
  border-width: 0 0 0 1rpx;
}
/*箭头*/
.arrows {
  position: relative;
}
.arrows.arrows-up::after {
  transform: rotate(-45deg);
}
.arrows.arrows-down::after {
  transform: rotate(135deg);
}
.arrows::after {
  content: "";
  display: inline-block;
  width: 6px;
  height: 6px;
  border: 2px solid #c3c3c3;
  border-width: 2px 2px 0 0;
  position: absolute;
  top: 50%;
  right: 26rpx;
  margin-top: -3px;
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
}
.arrows-inline::after {
  position: relative;
  left: auto;
  top: auto;
  right: auto;
  bottom: auto;
  margin-top: -4px;
  margin-left: 5px;
}
